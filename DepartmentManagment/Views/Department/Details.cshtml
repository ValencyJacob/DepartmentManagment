@model Department

<div class="container">

    <div class="card">
        <div class="card-header">
            @Model.Name
        </div>
        <div class="card-body">
            <blockquote class="blockquote mb-0">
                <p>
                    Description
                </p>

                <footer class="blockquote-footer">
                    Divisions:
                    
                    @for (int i = 0; i < Model.Divisions.Count; i++)
                    {
                        <p>
                            @i) @Model.Divisions[i].Name 
                        </p>
                    }
                </footer>

            </blockquote>
        </div>
    </div>

    <a asp-action="Index" class="btn btn-primary">
        Back
    </a>

</div> 