@model Division

<div class="container">

    <div class="card">
        <div class="card-header">
            @Model.Name
        </div>
        <div class="card-body">
            <blockquote class="blockquote mb-0">
                <p>
                    Description
                </p>
                <footer class="blockquote-footer">Department: <cite title="Source Title">@Model.Department.Name</cite></footer>
            </blockquote>
        </div>
    </div>

    <a asp-action="Index" class="btn btn-primary">
        Back
    </a>

</div>  